{{#component "icons.websocketSvg" fillColor="currentColor" strokeColor="currentColor" size="64"}}
    <svg width="{{size}}" height="{{size}}" viewBox="0 0 128 128" fill="none" xmlns="http://www.w3.org/2000/svg">
        <!-- Outlet container -->
        <rect x="6" y="23" width="116" height="83" rx="55" stroke="{{strokeColor}}" stroke-width="12" fill="none"/>
        <!-- Left socket (eye) -->
        <rect x="41" y="48" width="6" height="20" rx="3" stroke="{{strokeColor}}" stroke-width="5" fill="{{fillColor}}"/>
        <!-- Right socket (eye) -->
        <rect x="81" y="48" width="6" height="20" rx="3" stroke="{{strokeColor}}" stroke-width="5" fill="{{fillColor}}"/>
        <!-- Ground hole (mouth) -->
        <circle cx="64" cy="80" r="6" stroke="{{strokeColor}}" stroke-width="5" fill="{{fillColor}}"/>
    </svg>
{{/component}}

