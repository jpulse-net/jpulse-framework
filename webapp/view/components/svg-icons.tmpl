{{#component "icons.systemStatusSvg" fillColor="currentColor" strokeColor="currentColor" size="64"}}
    <svg width="{{size}}" height="{{size}}" viewBox="0 0 128 128" fill="none" xmlns="http://www.w3.org/2000/svg" stroke="{{strokeColor}}" stroke-width="10" stroke-linecap="round" stroke-linejoin="round">
        <!-- Server/System representation -->
        <rect x="10" y="16" width="108" height="76" rx="8" ry="8" stroke-width="12" />
        <line x1="42" y1="112" x2="86" y2="112" />
        <line x1="64" y1="92" x2="64" y2="112" stroke-width="12" />
        <!-- Status indicators -->
        <circle cx="32" cy="38" r="4" fill="{{fillColor}}" />
        <circle cx="54" cy="38" r="4" fill="{{fillColor}}" />
        <circle cx="76" cy="38" r="4" fill="{{fillColor}}" />
        <!-- Performance graph lines -->
        <polyline points="26,75 42,65 64,70 86,55 102,60" stroke="{{strokeColor}}" />
    </svg>
{{/component}}

